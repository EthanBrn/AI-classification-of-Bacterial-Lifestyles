  {
   "cell_type": "code",
   "execution_count": 400,
   "id": "878d0a6a",
   "metadata": {},
   "outputs": [],
   "source": [
    "v_arr = np.zeros(len(data)-1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 401,
   "id": "e613dfcb",
   "metadata": {},
   "outputs": [],
   "source": [
    "v_arr = np.zeros(len(data))\n",
    "t_arr = np.zeros(len(data)-1)\n",
    "\n",
    "for i in range(1,len(data)-1):\n",
    "    \n",
    "    t_next = time.iloc[i+1]\n",
    "    x_next = x_smooth.iloc[i+1]\n",
    "    y_next = y_smooth.iloc[i+1]\n",
    "    z_next = z_smooth.iloc[i+1]\n",
    "    \n",
    "    t_prev = time.iloc[i-1]\n",
    "    x_prev = x_smooth.iloc[i-1]\n",
    "    y_prev = y_smooth.iloc[i-1]\n",
    "    z_prev = z_smooth.iloc[i-1]\n",
    "    \n",
    "    # Calculate radial distance for next and previous points\n",
    " \n",
    "    \n",
    "    \n",
    "    r_diff = np.sqrt((x_next-x_prev)**2+(y_next-y_prev)**2+(z_next-z_prev)**2)\n",
    "    dt = t_next - t_prev\n",
    "  \n",
    "  \n",
    "    #velocity at the current point using central difference\n",
    "    v = r_diff/ (dt)\n",
    "\n",
    "    v_arr[i] = v\n",
    "    t_arr[i]= dt\n",
    "    \n",
    "    \n",
    "   "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 402,
   "id": "43439904",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "17.17633772913912"
      ]
     },
     "execution_count": 402,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "np.mean(v_arr)"
   ]
  },
