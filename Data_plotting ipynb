    "\n",
    "# Add color bar to show velocity scale\n",
    "cbar = plt.colorbar(scatter, ax=ax, shrink=0.6)\n",
    "cbar.set_label(r'Speed ($\\mu$m s$^{-1}$)')\n",
    "ax.set_title('Test Plot WT_planktonic Track1_0')\n",
